@inject AppInfoVo appInfoVo
@{
    Layout = "_Layout";
    var request = this.ViewContext.HttpContext.Request;
    ViewBag.currentPath = $"{request.PathBase}{request.Path}".TrimEnd('/').ToLower();
    ViewBag.appInfoVo = appInfoVo;
    ViewBag.viewPath = this.ViewContext.View.Path;
    ViewBag.routeData = this.ViewContext.RouteData;
}
